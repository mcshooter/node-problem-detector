{
	"plugin": "filelog",
	"pluginConfig": {
		"timestamp": "^time=\"(\\S*)\"",
		"message": "msg=\"([^\n]*)\"",
		"timestampFormat": "2006-01-02T15:04:05.999999999Z07:00"
	},
	"logPath": "C:\\containerd\\containerd.log",
	"lookback": "5m",
	"bufferSize": 10,
	"source": "containerd",
	"conditions": [],
	"rules": [
		{
			"type": "temporary",
			"reason": "MissingPigz",
			"pattern": "unpigz not found.*"
		},
		{
			"type": "temporary",
			"reason": "ImageCreate",
			"pattern": "ImageCreate event.*"
		},
		{
			"type": "temporary",
			"reason": "CRIStopped",
			"pattern": "Stop CRI service.*"
		},
		{
			"type": "temporary",
			"reason": "GCEvent",
			"pattern": "garbage collected.*"
		},
		{
			"type": "temporary",
			"reason": "IncompatibleContainer",
			"pattern": ".*CreateComputeSystem.*"
		},
		{
			"type": "temporary",
			"reason": "loading plugin",
			"pattern": "loading plugin.*"
		}
	]
}
